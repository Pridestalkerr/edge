{
  "name": "@edge-placeholder/eslint__experimental-ts",
  "type": "module",
  "scripts": {
    "clean": "rm -rf .turbo node_modules",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check '**/*.{ts,json}' --ignore-unknown --no-error-on-unmatched-pattern",
    "format:fix": "prettier --write '**/*.{ts,json}' --ignore-unknown --no-error-on-unmatched-pattern",
    "lint": "eslint . --cache --cache-location node_modules/.cache/.eslintcache --flag unstable_ts_config",
    "lint:fix": "pnpm lint --fix"
  },
  "prettier": "@edge-placeholder/prettier-config/base",
  "devDependencies": {
    "@edge-placeholder/prettier-config": "undefined",
    "@edge-placeholder/tsconfig": "undefined",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@next/eslint-plugin-next": "^15.1.0",
    "@types/eslint-plugin-jsx-a11y": "^6.10.0",
    "@types/eslint-plugin-tailwindcss": "^3.17.0",
    "@typescript-eslint/utils": "^8.18.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-import-x": "^4.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "globals": "^15.13.0",
    "jiti": "^2.4.1",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  }
}
